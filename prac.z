function fibonacci(n: int) -> int {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

function main_logic() -> int {
    let final_char: char = 'B';
    let condition: bool = true;
    if (condition) {
        final_char = 'T';
    }

    let final_value_check: int = fibonacci(5);
    return final_value_check;
}